#include "lists.h"
/**
 * pop_listint - frees a node list
 * @head: head node pointer
 * Description: frees a node list
 * Return: void
 */
listint_t *get_nodeint_at_index(listint_t *head, unsigned int index)
{
	listint_t *temp;

	listint_t i = 0, n;

	if (head == NULL)
		return (NULL);

	temp = head;
	while (i < index)
	{
		temp = temp->next;

		if  (temp = NULL)
			return (NULL);
		i++;
	}
	n = temp->n;
	return (n);
}
